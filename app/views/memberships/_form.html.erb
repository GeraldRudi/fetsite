<%= semantic_form_for [@membership.fetprofile,@membership] do |f| %>
  <%= f.inputs do %>
    <%= f.input :gremium, :as=>:radio %>
    <%= f.input :start %>
    <%= f.input :stop %>
    <%= f.input :typ ,:as=>:radio, :collection=>{"mitglied"=>1, "ersatzmitglied"=>2} %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
