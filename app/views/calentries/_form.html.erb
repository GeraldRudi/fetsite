<%= semantic_form_for @calentry do |f| %>
  <%= f.inputs do %>
    <%= f.input :start , :as => :date_string %>
    <%= f.input :ende %>
    <%= f.input :summary %>
    <%= f.input :calendar, :as=> :radio %>
    <%= f.input :typ %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
