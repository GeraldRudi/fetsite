<div class="container-fluid">
<%= semantic_form_for @calentry do |f| %>
  <%= f.inputs do %>
 <div class="row-fluid">
 <div class="span6">
    <%= f.input :start , :as => :date_string, :append=>'sadf'  %>
 </div>
 <div class="span6">
    <%= f.input :ende , :as => :datepicker %>
 </div>   
 </div>
 <div class="row-fluid">  
 <div class="span12">
    <%= f.input :summary, :append=>'g' %>
 </div>
 </div>
 <div class="row-fluid">
 <div class="span6">
    <%= f.input :calendars, :as=> :radio %>
 </div>
 <div class="span6">
    <%= f.input :typ %>
 </div>
 </div>
  <% end %>
  <div class="row-fluid">
		  <script>$('.datepicker').datepicker()</script>  

  <div class="span12">
  <%= f.actions do %>
    <%= f.action :submit, :as => :input_ %>
  <% end %>
  </div>
  </div>
<% end %>
</div>

