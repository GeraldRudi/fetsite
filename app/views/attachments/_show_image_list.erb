<% if images.size > 0 %>
<ul>
  <% images.each do |image| %>
    <li>
      <a href="javascript:void(0)" onclick="<%="ts_insert_image('#{image.val.public_filename()}', '#{image.name}');" %>">
      <%= image_tag( image.val.public_filename(:thumb) ) %></a>
    </li>
  <% end %>
</ul>
<% else %>
 No Images Uploaded Yet.
<% end %>
