<div class="container-fluid">
  <%= render 'studien/tabs' %>
  <div class="row-fluid">
    <h1><%= I18n.t("studien.list") %></h1>


    <% @studien.each_slice(2) do |row| %>
    <div class="row-fluid">
    <% row.each do |studium|%>
      <div class="span6">
	<%= render studium%>
      </div>
    <% end %>
    </div>
    <%end%>

    <br>

  </div>
  <%= render :partial=>'layouts/pretty_toolbar'%>
</div>
